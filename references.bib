@article{mabon2022,
  title = {Whose Knowledge Counts in Nature-Based Solutions? {{Understanding}} Epistemic Justice for Nature-Based Solutions through a Multi-City Comparison across {{Europe}} and {{Asia}}},
  shorttitle = {Whose Knowledge Counts in Nature-Based Solutions?},
  author = {Mabon, Leslie and Barkved, Line and De Bruin, Karianne and Shih, Wan-Yu},
