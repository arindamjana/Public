  author = {Bai, Xuemei and Surveyer, Alyson and Elmqvist, Thomas and Gatzweiler, Franz W. and G{\"u}neralp, Burak and Parnell, Susan and {Prieur-Richard}, Anne H{\'e}l{\`e}ne and Shrivastava, Paul and Siri, Jos{\'e} Gabriel and {Stafford-Smith}, Mark and Toussaint, Jean Patrick and Webb, Robert},
  year = {2016},
