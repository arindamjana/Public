@article{hodson2012,
  title = {Reshaping {{Urban Infrastructure}}: {{Material Flow Analysis}} and {{Transitions Analysis}} in an {{Urban Context}}},
  author = {Hodson, Mike and Marvin, Simon and Robinson, Blake and Swilling, Mark},
  year = {2012},
  journal = {Journal of Industrial Ecology},
  volume = {16},
  number = {6},
