@article{grabowski2022,
  title = {How Deep Does Justice Go? {{Addressing}} Ecological, Indigenous, and Infrastructural Justice through Nature-Based Solutions in {{New York City}}},
  shorttitle = {How Deep Does Justice Go?},
  author = {Grabowski, Zbigniew Jakub and Wijsman, Katinka and Tomateo, Claudia and McPhearson, Timon},
  year = {2022},
