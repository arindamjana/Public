@article{abellocolak2023,
  title = {Framing Urban Threats: {{A}} Socio-Spatial Analysis of Urban Securitisation in {{Latin America}} and the {{Caribbean}}},
  shorttitle = {Framing Urban Threats},
  author = {Abello Colak, Alexandra and Lombard, Melanie and {Guarneros-Meza}, Valeria},
