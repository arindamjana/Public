@article{guerra2014,
  title = {Mexico {{City}}'s Suburban Land Use and Transit Connection: {{The}} Effects of the {{Line B Metro}} Expansion},
  shorttitle = {Mexico {{City}}'s Suburban Land Use and Transit Connection},
  author = {Guerra, Erick},
